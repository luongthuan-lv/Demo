        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".child_screen_two.SoTienChiActivity"></activity>
        <activity android:name=".child_screen_two.NgayChiActivity" />
        <activity android:name=".child_screen_two.LoaiChiActivity" />
        <activity android:name=".child_screen_one.SoTienThuActivity" />
        <activity android:name=".child_screen_one.NgayThuActivity" />
        <activity android:name=".child_screen_one.LoaiThuActivity" />
        <activity android:name=".screen.Sub2Activity" />
        <activity android:name=".screen.SubActivity" />
        <activity android:name=".screen.GioithieuActivity" />
        <activity android:name=".screen.ThongkeActivity" />
        <activity android:name=".child_screen_two.KhoanChiActivity" />
        <activity android:name=".child_screen_one.KhoanThuActivity" />
        <activity
            android:name=".screen.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>